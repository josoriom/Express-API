  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jest": "^28.1.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.6.5",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "prettier": "^2.7.1",
    "socket.io": "^4.5.3"
  }
