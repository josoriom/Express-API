{
  "name": "express-template",
  "version": "0.0.0",
  "description": "Express API template",
  "main": "src/index.js",
  "scripts": {
    "eslint": "eslint src",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test-only": "jest --coverage",
    "test": "npm run test-only && npm run eslint && npm run prettier"
  },
  "keywords": [
    "api"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "author": "josoriom",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "jest": "^28.1.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-config-google": "^0.14.0"
  }
}
