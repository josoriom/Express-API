    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "eslint-config-cheminfo": "^8.1.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.1"
